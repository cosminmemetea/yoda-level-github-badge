{
  "version": 2,
  "builds": [
    {
      "src": "api/badge.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/badge",
      "dest": "api/badge.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/badge.js": {
      "runtime": "nodejs18.x"
    }
  }
}